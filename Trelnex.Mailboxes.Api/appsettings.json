{
  "AllowedHosts": "*",
  "Auth": {
    "Auth": {
      "trelnex-api-mailboxes": {
        "Instance": "https://login.microsoftonline.com/",
        "TenantId": "FROM_ENV",
        "ClientId": "FROM_ENV",
        "Audience": "FROM_ENV",
        "Scope": "api"
      }
    }
  },
  "AzureCredentials": {
    "Sources": [ "AzureCli" ]
  },
  "Clients": {
    "Groups": {
      "CredentialProviderName": "AzureCredentialProvider",
      "Scope": "FROM_ENV",
      "BaseUri": "FROM_ENV"
    },
    "Users": {
      "CredentialProviderName": "AzureCredentialProvider",
      "Scope": "FROM_ENV",
      "BaseUri": "FROM_ENV"
    }
  },
  "CosmosCommandProviders": {
    "TenantId": "FROM_ENV",
    "EndpointUri": "FROM_ENV",
    "DatabaseId": "trelnex-mailboxes",
    "Containers": [
      {
        "TypeName": "mailbox",
        "ContainerId": "mailboxes"
      },
      {
        "TypeName": "group-mailbox",
        "ContainerId": "group-mailboxes"
      },
      {
        "TypeName": "user-mailbox",
        "ContainerId": "user-mailboxes"
      }
    ]
  },
  "RewriteRules": [
    {
      "Regex": "^mailboxes-service/(.*)",
      "Replacement": "$1",
      "SkipRemainingRules": "true"
    }
  ],
  "Swagger": {
    "ServiceName": "Mailboxes"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Expressions"
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "StartsWith(RequestPath, '/healthz') or StartsWith(RequestPath, '/swagger')"
        }
      }
    ]
  }
}