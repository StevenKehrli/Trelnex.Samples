{
  "AllowedHosts": "*",
  "Auth": {
    "Auth": {
      "trelnex-api-groups": {
        "Instance": "https://login.microsoftonline.com/",
        "TenantId": "FROM_ENV",
        "ClientId": "FROM_ENV",
        "Audience": "FROM_ENV",
        "Scope": "groups"
      }
    }
  },
  "CredentialFactory": {
    "Sources": [ "AzureCli" ]
  },
  "CosmosCommandProviders": {
    "TenantId": "FROM_ENV",
    "EndpointUri": "FROM_ENV",
    "DatabaseId": "trelnex-groups",
    "Containers": [
      {
        "TypeName": "group",
        "ContainerId": "groups"
      }
    ]
  },
  "RewriteRules": [
    {
      "Regex": "^groups-service/(.*)",
      "Replacement": "$1",
      "SkipRemainingRules": "true"
    }
  ],
  "Swagger": {
    "ServiceName": "Groups"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Expressions"
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "StartsWith(RequestPath, '/healthz') or StartsWith(RequestPath, '/swagger')"
        }
      }
    ]
  }
}